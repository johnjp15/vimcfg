syntax enable
colorscheme monokai
set number	"line numbers

"indentation
set autoindent
filetype plugin indent on

set tabstop=4
set shiftwidth=4
set softtabstop=4
set expandtab   "this sets vimt o use spaces instead of tabs; use for Amazon

set backspace=indent,eol,start

set term=xterm-256color
set term=screen-256color
set t_ut=

set hlsearch
